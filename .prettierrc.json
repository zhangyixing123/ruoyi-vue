{
    "printWidth": 100,
    "Semicolons": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "lf",
    "jsxSingleQuote": false,
    "tabWidth": 2,
    "semi": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "babel",
    "overrides": [{
        "files": "*.vue",
        "options": {
            "parser": "vue"
        }
    }]
}